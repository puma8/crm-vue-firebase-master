{
  "name": "mavrik-crm",
  "version": "0.0.1",
  "description": "Mavrik CRM",
  "productName": "Mavrik CRM",
  "cordovaId": "org.cordova.quasar.app",
  "author": "lionking <lionking71685@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "run-emulator": "firebase emulators:start --only firestore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:dbrule": "export FIRESTORE_EMULATOR_HOST=localhost:5002 & mocha",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.2.0",
    "axios": "^0.19.0",
    "deepmerge": "^3.3.0",
    "firebase": "^6.6.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "qs": "^6.7.0",
    "quasar": "1.1.7",
    "quasar-dotenv": "^1.0.4",
    "vue-avatar": "^2.1.7",
    "vue-fragment": "^1.5.1",
    "vue-i18n": "^7.3.3",
    "vue-smooth-dnd": "^0.8.0",
    "vuefire": "^2.0.0",
    "vuelidate": "^0.7.4",
    "vuexfire": "^3.0.0"
  },
  "devDependencies": {
    "@firebase/testing": "^0.11.5",
    "@quasar/app": "^1.0.4",
    "@quasar/quasar-app-extension-colorize": "^1.0.0-alpha.1",
    "@quasar/quasar-app-extension-qcalendar": "^1.0.7",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "filesystem": "^1.0.1",
    "mocha": "^6.1.4",
    "prettier-eslint": "^9.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
